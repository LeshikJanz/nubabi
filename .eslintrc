{
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "rules": {
    "no-use-before-define": 0,
    "no-underscore-dangle": [2,  { "allowAfterThis": true }],
    "arrow-body-style": 0,
    "class-methods-use-this": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0
  }
}