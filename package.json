    "react-native-video": "^1.0.0",
    "react-native-video-player": "^0.8.0",
    "react-native-video-thumbnail": "^1.0.1",
    "react-navigation": "https://github.com/blackxored/react-navigation#feat/allow-hiding-floating-headers",
    "react-redux": "^5.0.2",
    "reading-time": "^1.1.0",
    "recompose": "^0.23.1",
