    "eslint-plugin-react-native": "^2.3.1",
    "express": "^4.15.3",
    "firebase-admin": "^5.0.1",
    "flow-bin": "^0.47.0",
    "graphql-server-express": "^1.0.2",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "lint-staged": "^3.4.0",
    "react-dom": "16.0.0-alpha.12",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-devtools": "^3.3.2",
    "redux-mock-store": "^1.2.3"
  }
}
