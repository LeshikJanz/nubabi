    "react-native-sound": "https://github.com/blackxored/react-native-sound#feat/preserve-encoding",
    "react-native-spinkit": "^1.0.2",
    "react-native-svg": "^5.2.0",
    "react-native-tab-navigator": "^0.3.3",
    "react-native-uuid": "^1.4.9",
