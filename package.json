{
  "name": "NuBabiMobile",
  "version": "1.0.0-beta.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "flow": "flow",
    "test": "jest",
    "prettier": "prettier --write --trailing-comma all --tab-width 2 --single-quote --parser flow --print-width 80 \"src/**/*.js\"",
    "precommit": "lint-staged",
    "changelog": "conventional-changelog -i CHANGELOG.md -s",
    "firebase:install": "cd src/server/firebase/functions && npm install",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "firebase:deploy:rules": "firebase deploy --only database",
    "server": "babel-node src/server/graphql/server.js",
    "server:watch": "BABEL_DISABLE_CACHE=1 nodemon --exec \"DEBUG=graphqlServer:*,axios babel-node\" -e js,graphqls src/server/graphql/server.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --trailing-comma all --tab-width 2 --single-quote --parser flow --print-width 80",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/flow-typed/**"
    ],
    "snapshotSerializers": [
      "<rootDir>/src/native/shared/felaSnapshotSerializer"
    ]
  },
  "quokka": {
    "babel": true
  },
  "dependencies": {
    "apollo-client": "^1.0.1",
    "apollo-test-utils": "^0.3.0",
    "art": "^0.10.1",
    "axios": "^0.16.0",
    "axios-debug-log": "^0.3.0",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-define": "https://github.com/blackxored/babel-plugin-transform-define#react-native-packager-fix",
    "base-64": "^0.1.0",
    "color": "^1.0.3",
    "d3-array": "^1.2.0",
    "d3-scale": "^1.0.5",
    "d3-shape": "^1.0.6",
    "dataloader": "^1.3.0",
    "faker": "^4.1.0",
    "fela": "^4.3.5",
    "fela-native": "^4.3.2",
    "firebase": "3.7.4",
    "graphql": "^0.9.2",
    "graphql-anywhere": "^3.0.1",
    "graphql-iso-date": "^3.1.0",
    "graphql-relay": "https://github.com/blackxored/graphql-relay-js#rn-base64",
    "graphql-tag": "^2.0.0",
    "graphql-tools": "^0.11.0",
    "hoist-non-react-statics": "^1.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "pluralize": "^4.0.0",
    "prop-types": "^15.5.8",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "raven-js": "^3.14.0",
    "react": "16.0.0-alpha.12",
    "react-apollo": "^1.4.2",
    "react-fela": "^4.3.5",
    "react-native": "0.46.2",
    "react-native-audio": "^3.2.2",
    "react-native-cached-image": "^1.3.3",
    "react-native-contacts-wrapper": "^0.2.3",
    "react-native-datepicker": "^1.5.1",
    "react-native-dropdownalert": "^2.7.1",
    "react-native-image-crop-picker": "^0.14.1",
